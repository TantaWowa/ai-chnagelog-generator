{
  "name": "@tantawowa/ai-changelog-generator",
  "version": "1.0.0",
  "description": "An npm command line tool that generates changelogs in keep-a-changelog format using OpenAI or XAI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "aic": "bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "ts-node src/cli.ts",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "changelog",
    "ai",
    "openai",
    "xai",
    "keep-a-changelog",
    "cli",
    "generator"
  ],
  "author": "Tantawowa",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tantawowa/ai-changelog-generator.git"
  },
  "bugs": {
    "url": "https://github.com/tantawowa/ai-changelog-generator/issues"
  },
  "homepage": "https://github.com/tantawowa/ai-changelog-generator#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/tantawowa"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "fs-extra": "^11.3.2",
    "openai": "^6.1.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
